    "csurf": "^1.11.0",
    "dockerfile-ast": "^0.5.0",
    "dotenv": "^16.0.2",
    "express-openid-connect": "^2.8.0",
    "firecracker-client": "./deps/firecracker-client",
    "flowbite": "^1.6.2",
    "flowbite-react": "0.4.2",
    "fs-ext": "^2.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "ms": "^2.1.3",
    "netmask": "^2.0.2",
    "node-ssh": "^13.0.0",
    "path-browserify": "^1.0.1",
    "prisma": "4.7.1",
    "proper-lockfile": "^4.1.2",
    "react": "^18.2.0",
